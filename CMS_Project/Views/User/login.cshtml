@model CMS_Project.Models.User
@{
    ViewBag.Title = "login";
}

<h2>login</h2>
@using (Html.BeginForm())
{
    @Html.ValidationSummary()
    <div>
        @Html.LabelFor(q => q.username)
        @Html.TextBoxFor(q => q.username)
    </div>
    <div>
        @Html.LabelFor(q => q.password)
        @Html.PasswordFor(q => q.password)
    </div>
    <input type="submit" value="Log in" />
}